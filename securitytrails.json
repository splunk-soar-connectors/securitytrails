                        "whois_street4",
                        "whois_telephone",
                        "whois_postalCode",
                        "whois_organization",
                        "whois_name",
                        "whois_fax",
                        "whois_city"
                    ],
                    "order": 0
                },
                "filterstring": {
                    "description": "Filter string to search",
                    "data_type": "string",
                    "required": true,
                    "order": 1
                },
                "keyword": {
                    "description": "Keyword to search corresponding filter results",
                    "data_type": "string",
                    "required": false,
                    "order": 2
                }
